{
  "name": "@craigmcc/cityteam-guests",
  "version": "1.0.0",
  "private": true,
  "author": "Craig McClanahan <craigmcc@gmail.com>",
  "description": "CityTeam Guests Checkin Application (server and client)",
  "main": "dist/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/craigmcc/cityteam-guests.git"
  },
  "scripts": {
    "develop:dev": "concurrently \"npm:server:build:watch\" \"npm:start:dev\"",
    "develop:prod": "concurrently \"npm:server:build:watch\" \"npm:start:prod\"",
    "run": "NODE_ENV=production node dist/server.js",
    "run:dev": "NODE_ENV=development node dist/server.js",
    "run:prod": "NODE_ENV=production node dist/server.js",
    "server:build": "tsc",
    "server:build:watch": "tsc --watch --preserveWatchOutput",
    "start:dev": "NODE_ENV=development nodemon dist/server.js",
    "start:prod": "NODE_ENV=production nodemon dist/server.js",
    "test": "NODE_ENV=test mocha -r ts-node/register src/**/*.test.ts",
    "test:watch": "NODE_ENV=test mocha --watch -r ts-node/register src/**/*.test.ts",
    "tsc": "tsc"
  },
  "keywords": [
    "cityteam",
    "guests",
    "bootstrap",
    "express",
    "javascript",
    "node",
    "postgres",
    "sequelize",
    "typescript"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@craigmcc/oauth-orchestrator": "^1.0.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csvtojson": "^2.0.10",
    "custom-env": "^2.0.1",
    "express": "^5.0.0-alpha.8",
    "morgan": "^1.10.0",
    "neat-csv": "^6.0.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.22.3",
    "sequelize-typescript": "^1.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.2.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.14",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.1",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "sqlite3": "^5.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
